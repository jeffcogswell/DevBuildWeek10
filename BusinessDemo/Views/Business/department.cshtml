@model List<Employee>
@{
    ViewData["Title"] = "department";
}

<h1>@ViewData["DeptName"]</h1>

@foreach(Employee emp in Model)
{
    <div><a href="/business/employee?emp=@emp.id">@emp.firstname @emp.lastname</a>
    
        <a href="/business/delete?id=@emp.id" class="btn btn-danger">Delete</a>
        <a href="/business/editform?empid=@emp.id" class="btn btn-primary">Edit</a>
    
    </div>
}